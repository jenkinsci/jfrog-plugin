pipeline {
    agent any
    tools {
        jfrog '${JFROG_CLI_TOOL_NAME_1}'
    }
    stages {
        stage ('Testing') {
            steps {
                // Show the installed version of JFrog CLI.
                jf '-v'

                jf 'c use ${TEST_CONFIGURED_SERVER_ID}'

                // Ping Artifactory.
                jf 'rt ping'

                // Upload a file to a repository in Artifactory
                jf 'rt u ${DUMMY_FILE_PATH} ${LOCAL_REPO}/'
            }
        }
    }
}